<div class="container">
  <hr>
  <div class="row">
    <div class="col-md-3 well sidebar-well">
      <h4> My Personal Database</h4>
      <i class="fa fa-th-large"></i>
      <%=link_to "View As Thumbnails", songs_path%>

    </div>
    <div class="col-md-9">
      <table class="table table-striped table-bordered table-hover table-condensed">
        <tr>
          <th class="center">Image</th>
          <th class="center">Name</th>
          <th class="center">Artist</th>
          <th class="center">Album</th>
          <th class="center">My Rating</th>
        </tr>
        
        <%@songs.each do |mysong| %>    
          <tr>
            <td class="center"><%= image_tag mysong.artwork %></td>
            <td class="center"><%=mysong.name%></td>
            <td class="center"><%=mysong.artist%></td>
            <td class="center"><%=mysong.songalbum%></td>
            <td class="center"><%= rating_for mysong, "rating", :enable_half => true, :disable_after_rate => false %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>


